<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ScrimInsetsView">
        <attr name="insetForeground" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="ColorTrackView">
        <attr name="ctvText" format="string"/>
        <attr name="ctvText_size" format="dimension"/>
        <attr name="ctvText_origin_color" format="color"/>
        <attr name="ctvText_change_color" format="color"/>
        <attr name="ctvProgress" format="float"/>
        <attr name="ctvDirection" format="integer"/>
    </declare-styleable>

    <declare-styleable name="LineTextView">
        <attr name="lineWidth" format="dimension" />
        <attr name="lineColor" format="color" />
        <attr name="animationDuration" format="integer" />
    </declare-styleable>

    <declare-styleable name="IndexBar">
        <attr name="indexTextSize" format="dimension"/>
        <attr name="indexTextColor" format="color"/>
        <attr name="indexSelectedTextColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="SectionItemDecoration">
        <attr name="sectionColor" format="color"/>
        <attr name="sectionHeight" format="dimension|reference"/>
        <attr name="sectionTextColor" format="color"/>
        <attr name="sectionTextSize" format="dimension|reference"/>
    </declare-styleable>
</resources>